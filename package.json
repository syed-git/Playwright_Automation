{
  "name": "playwright_automation",
  "version": "1.0.0",
  "description": "This repository is designed for automation testing using playwright tool",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test-parallel": "npmx playwright test --workers 5",
    "regression": "npx playwright test -g '@regression'",
    "smoke": "npx playwright test -g '@smoke'",
    "test-tag": "playwright test --grep",
    "lint": "eslint ./tests/**/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syed-git/Playwright_Automation.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/syed-git/Playwright_Automation/issues"
  },
  "homepage": "https://github.com/syed-git/Playwright_Automation#readme",
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@types/node": "^22.10.10",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-define-config": "^2.1.0"
  },
  "dependencies": {
    "moment": "^2.30.1"
  }
}
